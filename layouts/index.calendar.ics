BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//fusion-dancing-eu//NONSGML v1.0//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
{{ range .Site.Data.events.events }}
BEGIN:VEVENT
UID:{{ .name | urlize }}@fusion-dancing.eu
SUMMARY:{{ .name }}
DTSTART;VALUE=DATE:{{ .start | dateFormat "20060102" }}
DTEND;VALUE=DATE:{{ .end | dateFormat "20060102" }}
LOCATION:{{ .location }}
URL:{{ .link }}
DESCRIPTION:Join us at {{ .name }} in {{ .location }}! More info: {{ .link }}
END:VEVENT
{{ end }}
END:VCALENDAR